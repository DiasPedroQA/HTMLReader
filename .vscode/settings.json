    "python.testing.pytestEnabled": true,
    "python.testing.unittestEnabled": false,
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.cwd": "${workspaceFolder}",
    "python.testing.pytestPath": "${workspaceFolder}/.venv/bin/pytest",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        // "**/__init__.py": true,
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/__pycache__": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "makefile.makefilePath": "Makefile",
    "makefile.buildLog": ".logs_makefile",
    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${workspaceFolder}/src"
    },
    "python.envFile": "${workspaceFolder}/.env",
    "github-actions.workflows.pinned.workflows": [],
    "makefile.alwaysPostConfigure": true,
    "makefile.alwaysPreConfigure": true,
    "makefile.configureOnOpen": true,
    "makefile.loggingLevel": "Verbose",
    "editor.defaultFormatter": "ms-python.black-formatter",
    "notebook.defaultFormatter": "ms-python.black-formatter",
    "editor.defaultFoldingRangeProvider": "ms-python.black-formatter"
